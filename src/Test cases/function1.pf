{	function1  --  part of verifier test suite				Version 1.2 of 7/7/82}program function1;var i: integer;    tab: array[1..100] of integer;{	successor  -  add one to input}function successor(n: integer): integer;entry n < 32767;exit return = n.old + 1;begin    successor := n + 1;end;begin    i := 25;    tab[i+5] := 3;    i := successor(tab[i + 5]);    assert(i = 4);end.