##	Pending outgoing mail printer##	Finds outgoing mail of current user not yet sent and prints headers.##						version 1.3 of 9/30/82SPOOL=/usr/spool/unetmail/*#	Examine all files in spool filefor FILE in $SPOOL#	If file is readable    do    if test -r $FILE#	Find out if file is from current user    then if grep -s "From: ${USER}\@" $FILE#	If so, print its mail headers        then head $FILE | grep ": "	    echo ""	fi    fidone